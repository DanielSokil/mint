record X {
  name : String
}

component Main {
  fun render : String {
    x.name
  } where {
    x = { name = "test" }
  }
}
--------------------------------------------------------------------------------
const A = _R({
  name: [
    "name",
    Decoder.string
  ]
});

class B extends _C {
  render() {
    let a = new A({
      name: `test`
    });

    return a.name;
  }
};

B.displayName = "Main";
