component Main {
  fun test : String {
    getName("Joe")
  } where {
    getName =
      (name : String) : String { name }
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  a() {
    let b = (c) => {
      return c
    };

    return b(`Joe`);
  }

  render() {
    return (() => {
      this.a();
      return ``;
    })();
  }
};

A.displayName = "Main";
