component Main {
  fun test : String {
    a("Joe")
  } where {
    a =
      (b : String) : String { b }
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  test() {
    let a = ((b) => {
      return b
    })

    return a(`Joe`)
  }

  render() {
    return (() => {
      this.test()

      return ``
    })()
  }
}

$Main.displayName = "Main"
