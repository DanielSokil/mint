module Promise {
  fun reject (input : a) : Promise(a, b) {
    ``
  }
}

component Main {
  fun test : Promise(Never, Void) {
    sequence {
      greeting =
        Promise.reject("hello")

      void
    } catch String => a {
      void
    }
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
const $Promise = new(class extends Module {
  reject(input) {
    return
  }
})

class $Main extends Component {
  test() {
    return (async () => {
      let _result = null;

      try {
        let greeting = await (async () => {
          try {
            return await $Promise.reject(`hello`)
          } catch (_error) {
            let a = _error;

            _result = null

            throw new DoError()
          }
        })()

        _result = await null
      } catch (_error) {
        if (_error instanceof DoError) {} else {
          console.warn(`Unhandled error in sequence expression:`)
          console.warn(_error)
        }
      }

      return _result
    })()
  }

  render() {
    return (() => {
      this.test()

      return ``
    })()
  }
}

$Main.displayName = "Main"
