component Main {
  get test : String {
    something
  } where {
    something =
      "Asd"

    otherThing =
      0
  }

  fun render : String {
    test
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  get a() {
    let b = `Asd`
    return b
  }

  render() {
    return this.a
  }
}

A.displayName = "Main"
