component Main {
  get test : String {
    something
  } where {
    something =
      "Asd"

    otherThing =
      0
  }

  fun render : String {
    test
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  get test() {
    let something = `Asd`

    return something
  }

  render() {
    return this.test
  }
}

$Main.displayName = "Main"
