component Main {
  get test : String {
    x
  } where {
    x =
      "Hello"

    y =
      "Blah"
  }

  fun render : String {
    test
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  get a() {
    let b = `Hello`
    return b
  }

  render() {
    return this.a
  }
}

A.displayName = "Main"
