component Main {
  property name : String = "Joe"

  fun render : Html {
    <div/>
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  get name() {
    if (this.props.name != undefined) {
      return this.props.name
    } else {
      return `Joe`
    }
  }

  render() {
    return _createElement("div", {})
  }
}

$Main.displayName = "Main"
