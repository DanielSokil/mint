component Main {
  property test : String = "Hello"

  fun render : String {
    test
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  get test() {
    if (this.props.test != undefined) {
      return this.props.test
    } else {
      return `Hello`
    }
  }

  render() {
    return this.test
  }
}

$Main.displayName = "Main"
