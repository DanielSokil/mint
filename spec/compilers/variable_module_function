module A {
  fun a : String {
    "Hello"
  }

  fun b : Function(String) {
    a
  }
}

component Main {
  fun render : String {
    a()
  } where {
    a = A.b()
  }
}
--------------------------------------------------------------------------------
const $A = new(class {
  a() {
    return `Hello`
  }

  b() {
    return $A.a.bind($A)
  }
})

class $Main extends Component {
  render() {
    let a = $A.b()

    return a()
  }
}

$Main.displayName = "Main"

