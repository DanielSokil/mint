component Main {
  fun test : Array(String) {
    [
      "Hello",
      "Blah",
      "Joe"
    ]
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  test() {
    return [`Hello`, `Blah`, `Joe`]
  }

  render() {
    return (() => {
      this.test.bind(this)()

      return ``
    })()
  }
}

$Main.displayName = "Main"
