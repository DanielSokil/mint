component Main {
  fun a : String {
    "test"
  }

  fun test : String {
    a()
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  a() {
    return `test`
  }

  test() {
    return this.a.bind(this)()
  }

  render() {
    return (() => {
      this.test.bind(this)()

      return ``
    })()
  }
}

$Main.displayName = "Main"
