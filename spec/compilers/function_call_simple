component Main {
  fun a : String {
    "test"
  }

  fun test : String {
    a()
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  a() {
    return `test`
  }

  test() {
    return this.a()
  }

  render() {
    return (() => {
      this.test()

      return ``
    })()
  }
}

$Main.displayName = "Main"
