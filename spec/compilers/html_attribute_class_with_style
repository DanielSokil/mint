component Main {
  style base {
    width: 100%;
  }

  fun render : Html {
    <div::base class="something">
    </div>
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  render() {
    return _createElement("div", {
      className: `something` + ` main-base`
    })
  }
}

$Main.displayName = "Main"

_insertStyles(`
  .main-base {
    width: 100%;
  }
`)
