component Main {
  fun test (a : String) : String {
    a
  }

  fun render : String {
  	test("X")
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  test(a) {
    return a
  }

  render() {
    return this.test.bind(this)(`X`)
  }
}

$Main.displayName = "Main"
