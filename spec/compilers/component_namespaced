component Ui.Dropdown {
  fun render : String {
    "test"
  }
}

component Main {
  fun render : Html {
    <Ui.Dropdown/>
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return `test`
  }
}

A.displayName = "Ui.Dropdown"

class B extends _C {
  render() {
    return _h(A, {})
  }
}

B.displayName = "Main"
