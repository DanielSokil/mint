component Main {
  fun test : String {
    a
  } where {
    a =
      "Hello"
  }

  fun render : String {
    test()
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  a() {
    let b = `Hello`
    return b
  }

  render() {
    return this.a()
  }
}

A.displayName = "Main"

