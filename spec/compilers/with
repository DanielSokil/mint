module X {
  fun a : String {
    ""
  }
}

component Main {
  fun render : String {
    with X {
      a()
    }
  }
}
--------------------------------------------------------------------------------
const $X = new(class {
  a() {
    return ``
  }
})

class $Main extends Component {
  render() {
    return $X.a.bind($X)()
  }
}

$Main.displayName = "Main"
