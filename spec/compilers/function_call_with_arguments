component Main {
  fun call (a : String, b : Bool) : Bool {
    b
  }

  fun test : Bool {
    call("Hello", true)
  }

  fun render : String {
    try {
      test()

      ""
    }
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  call(a, b) {
    return b
  }

  test() {
    return this.call(`Hello`, true)
  }

  render() {
    return (() => {
      this.test()

      return ``
    })()
  }
}

$Main.displayName = "Main"

