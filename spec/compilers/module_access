module Test {
  fun a : String {
    "Hello"
  }

  fun b : Function(String) {
    Test.a
  }
}

component Main {
  fun render : String {
    x()
  } where {
    x =
      Test.b()
  }
}
--------------------------------------------------------------------------------
const $Test = new(class {
  a() {
    return `Hello`
  }

  b() {
    return $Test.a.bind($Test)
  }
})

class $Main extends Component {
  render() {
    let x = $Test.b()

    return x()
  }
}

$Main.displayName = "Main"
