component Main {
  fun test : Html {
    <div>
      <{ something }>
    </div>
  } where {
    something =
      "Asd"

    otherThing =
      0
  }

  fun render : Html {
    test()
  }
}
--------------------------------------------------------------------------------
class $Main extends Component {
  test() {
    let something = `Asd`

    return _createElement("div", {}, [something])
  }

  render() {
    return this.test.bind(this)()
  }
}

$Main.displayName = "Main"
