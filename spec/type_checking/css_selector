component Main {
  style test {
    & div {
      color: {color};
    }

    &:focus {
      color: red;
    }
  }

  get color : String {
    "blue"
  }

  fun render : Html {
    <div::test/>
  }
}
-------------------------------------------------------CssDefinitionTypeMismatch
component Main {
  style test {
    & div {
      color: {color};
    }
    &:focus {
      color: red;
    }
  }

  get color : Bool {
    true
  }

  fun render : Html {
    <div::test/>
  }
}
