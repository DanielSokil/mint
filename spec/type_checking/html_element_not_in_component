component Main {
  style base {
    color: red;
  }

  fun render : Html {
    <div::base>
    </div>
  }
}
----------------------------------------------HtmlElementStyleOutsideOfComponent
module X {
  fun render : Html {
    <div::base/>
  }
}

component Main {
  fun render : Html {
    X.render()
  }
}
